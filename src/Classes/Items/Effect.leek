class Effect{
	integer type = -1
	integer min = -1
	integer max = -1
	integer turns = -1
	integer targets = -1
	integer modifiers = -1

	constructor(Array<integer> arr){
		this.type = arr[0]
		this.min = arr[1]
		this.max = arr[2]
		this.turns= arr[3]
		this.targets = arr[4]
		this.modifiers = arr[5]
	}
}